        "multicolumn_AyYccf": {
            "type": "multicolumn",
            "blocks": {
                "column_4eMYpt": {
                    "type": "column",
                    "settings": {
                        "title": "Free Membership with Subscription",
                        "text": "<p>There’s no fee to join the Lollygagger Club -- <strong>membership is free with with a qualifying active subscription.<\/strong><\/p><ul><li>Subscribe to at least one bottle per quarter (four per year) to keep your membership active.<\/li><li>Up to 25% off<strong> ALL<\/strong> bottles, merchandise, and Hiatus events.<\/li><li>One weekly <strong>complimentary tasting <\/strong>for you and a guest.<\/li><\/ul>",
                        "link_label": "",
                        "link": ""
                    }
                },
                "column_34b69K": {
                    "type": "column",
                    "settings": {
                        "title": "More Member Benefits",
                        "text": "<p><strong>Exclusive Access<\/strong> to member-only events, distillery workshops, tours and tasting panels giving you a voice  in product development<\/p><p><strong>First Access<\/strong> to new products including  single-barrel bottlings.<\/p><p><strong>Earn Loyalty Points<\/strong> on all purchases. Get <strong>5 points for every $1 spent<\/strong>, with bonus points for referrals, birthdays, and social shares. Redeem your points for discounts on future purchases.<\/p>",
                        "link_label": "",
                        "link": ""
                    }
                },
                "column_rGUKTd": {
                    "type": "column",
                    "settings": {
                        "title": "Founding Members",
                        "text": "<p><strong>Join by December 31, 2025 <\/strong>to become part of the Founders Circle. Founders receive a <strong>custom Lollygaggers jersey<\/strong> and access to the <strong>Founders Party in early 2025<\/strong>.<\/p>",
                        "link_label": "",
                        "link": ""
                    }
                }
            },
